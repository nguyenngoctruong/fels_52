<li class="row">
  <div class="col-md-9"><%= link_to user.name, user %></div>
  <% unless user.admin? %>
    <div class="col-md-1"><%= link_to "Delete", admin_user_path(user),
      method: :delete, data: {confirm: "You sure?"} %></div>
  <% end %>
  <div class="col-md-1"><%= link_to "Edit", edit_admin_user_path(user) %></div>
</li>